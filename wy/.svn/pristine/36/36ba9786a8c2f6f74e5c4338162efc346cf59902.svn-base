@media only screen and (min-width: 1200px) {
	.sync{
		display: block;
		float: left;
		width:69px;
		height:22px;
		background: #3478F6;
		border-radius:11px;
		margin-right: 36px;
		text-align: center;
		color: #FFFFFF;
		font-size: 12px;
		margin-top: 26px;
	}
	.batch{
		display: block;
		float: left;
		width:69px;
		height:20px;
		line-height: 20px;
		border:1px solid #3478F6;
		border-radius:11px;
		color: #3478F6;
		text-align: center;
		font-size: 12px;
		margin-top: 26px;
	}

}
@media only screen and (max-width: 1200px) {
	.sync{
		display: block;
		float: left;
		width:111px;
		height:35px;
		line-height: 35px;
		background: #3478F6;
		border-radius:18px;
		margin-right: 50px;
		text-align: center;
		color: #FFFFFF;
		font-size: 18px;
		margin-top: 45px;
	}
	.batch{
		display: block;
		float: left;
		width:111px;
		height:35px;
		line-height: 35px;
		border:1px solid #3478F6;
		border-radius:18px;
		color: #3478F6;
		text-align: center;
		font-size: 18px;
		margin-top: 45px;
	}

}
