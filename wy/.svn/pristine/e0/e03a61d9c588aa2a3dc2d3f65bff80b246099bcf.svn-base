    }
    .order-desc-top .orderInfoTop{
        border:none;
    }
    .houseImg{
        width:100%;
        height:auto;
        overflow:hidden;
    }
    .houseImg img{
        width:280px;
        height:auto;
        overflow:hidden;
        border-radius:5px;
        margin:10px auto;
        display:block;
    }
    /**/
    .introBox{
        width:100%;
        height:auto;
        overflow:hidden;
        background:#fff;
        margin-top:12px;
    }
    .introBoxTop{
        width:100%;
        height:auto;
        overflow:hidden;
        margin:0 auto;
    }
    .introBoxTopLeft{
        width:auto;
        height:auto;
        overflow:hidden;
        font-size:14px;
        color:#666;
    }
    .introBoxTopLeft span{
        display:block;
        margin-top:5px;
        padding-left:40px;
    }
    .spanStyle{
        color:#3478F7;
        font-size:18px;
        cursor:pointer;
    }
    .calendarBoxText{
        font-size:18px;
        color:#3478F7;
        text-align:center;
        margin-top:10px;
        margin-bottom:10px;
    }
