        height:auto;
        overflow:hidden;
        padding-left:20px;
    }
    .orderStatus{
        font-size:18px;
        color:#666;
        display:block;
    }
    .orderInfoCount{
        width:auto;
        height:auto;
        overflow:hidden;
    }
    .orderInfoCount span{
        font-size:14px;
        color:#666;
        float:left;
    }
    .orderInfoCount span:last-child{
        margin-left:30px;
    }
    .cost{
        font-size:14px;
        color:#666;
        display:block;
    }
    .dialogBox{
        width:auto;
        height:auto;
        overflow:hidden;
        margin-top:35px;
    }
    .dialogBox .sellerImg{
        width:42px;
        height:42px;
        overflow:hidden;
        border-radius: 50%;
        float:right;
        margin-left:20px;
    }
    .dialogBox span{
        height:40px;
        overflow:hidden;
        line-height:40px;
        border:1px solid #666;
        padding:0px 32px;
        display:block;
        float:right;
        border-radius:20px;
    }
    .inputBox{
        width:auto;
        height:auto;
        overflow:hidden;
        margin-top:150px;
        border-top:1px solid #F1F2F7;
    }
    .inputBox img{
        width:28px;
        display:block;
        margin-top:8px;
        margin-left:30px;
    }
    .inputBox textarea{
        width:100%;
        height:50px;
        margin-top:15px;
        padding-left:30px;
    }
    .sure{
        height:25px;
        overflow:hidden;
        line-height:25px;
        padding:0 33px ;
        color:#fff;
        font-size:12px;
        border-radius:12px;
        float:right;
        margin-right:16px;
        background:#3478F7;
        display:block;
        margin-top:25px;
    }
    /**/
    /**/
    /*右边*/
    .calendarBox{
        width:360px;
        height:100%;
        overflow:hidden;
        float:left;
