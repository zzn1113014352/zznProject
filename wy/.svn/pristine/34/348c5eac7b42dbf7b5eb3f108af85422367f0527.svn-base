@media only screen and (max-width: 1200px) {
    /*.user-top{*/
        /*margin-top:27px;*/
    /*}*/
    .userManaSumLeft{
        margin-left:500px;
    }
    .userManaSum{
        width:auto;
        height:100%;
        overflow:hidden;
        float:left;
        margin-top:15px;
    }
    .userManaSumTopText span{
        font-size:14px;
    }
    .userManaSum img{
        width:23px;
        height:24px;
        overflow:hidden;
        padding-right:8px;
        border-right:1px solid #DFDFDF;
    }
    .userManaSumBottom{
        /*font-family:Adobe Heiti Std R;*/
        font-weight:normal;
        height:30px;
        overflow:hidden;
        margin-top:3px;
        margin-left:33px;
        font-size:55px;
        color:rgba(255,216,51,1);
        background:linear-gradient(0deg,rgba(255,255,255,1) 0%, rgba(255,255,255,1) 0%, rgba(156,166,255,1) 0%, rgba(126,100,255,1) 100%);
        -webkit-background-clip:text;
        -webkit-text-fill-color:transparent;
    }
    .userManaSumRight{
        margin-left:341px;
    }
    .buttonCommon{
        width:102px;
        height:40px;
        line-height:40px;
        font-size:18px;
        display:block;
        float:left;
        text-align:center;
        cursor:pointer;
    }
    .addUser{
        margin-left:55px;
    }
    .import{
        margin-left:30px;
    }
    .ifIndentify{
        width:130px;
        height:40px;
        line-height:40px;
        font-size:18px;
        display:block;
        float:left;
        text-align:center;
        background:#3478F6;
        border-radius:20px;
        margin-top:22px;
        color:#fff;
        margin-left:300px;
        cursor:pointer;
    }
    .write{
        width:295px!important;
        height:40px!important;
        line-height:40px!important;
        font-size:18px;
        display:block;
        float:left;
        border:1px solid #3478F6!important;
        border-radius:20px;
        margin-top:20px;
        color:#9CA6FF!important;
        /*margin-left:20px;*/
        padding-left:4px!important;
    }
    .notice-bottom .del{
        margin-right: 26px;
    }
    .notice-bottom .edit{
        width:23px;
        font-size:18px;
        color:rgba(91,91,91,1);
    }
    .notice-bottom .edit:hover{
        color: #3478F6;
    }
    .content-box .photo img{
        width:54px;
        height:54px;
        overflow:hidden;
        border-radius:50%;
    }
    .signal img{
        width:27px;
        height:27px;
        overflow:hidden;
    }
    /*弹窗*/
    .look-box ul li input{
        width:124px;
        height: 35px;
        overflow:hidden;
        border:1px solid #3478F7;
        border-radius:18px;
    }
    .address{
        width:124px;
        height: 35px;
        overflow:hidden;
        border:1px solid #3478F7;
        border-radius:18px;
    }
    .city{
        width:124px!important;
        height: 35px;
        overflow:hidden;
        display:block;
    }
    .city select{
        width:100%;
        height:100%;
        border:1px solid #3478F7;
        border-radius:18px;
    }
    .look-box .sure-btn{
        margin-top:65px;
    }
    .editUserTan{
        height:495px;
    }
    .address{
        height:80px!important;
    }
    /*弹窗*/
}
